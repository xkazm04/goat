7→>(({ className, ...props }, ref) => (\n    48→  <AccordionPrimitive.Item\n    49→    ref={ref}\n    50→    className={cn('border-b', className)}\n    51→    {...props}\n    52→  />\n    53→));\n    54→AccordionItem.displayName = 'AccordionItem';\n    55→\n    56→const AccordionTrigger = React.forwardRef<\n    57→  React.ElementRef<typeof AccordionPrimitive.Trigger>,\n    58→  React.ComponentPropsWithoutRef<typeof AccordionPrimitive.Trigger>\n    59→>(({ className, children, ...props }, ref) => (\n    60→  <AccordionPrimitive.Header className=\"flex\">\n    61→    <AccordionPrimitive.Trigger\n    62→      ref={ref}\n    63→      className={cn(\n    64→        'flex flex-1 items-center justify-between py-4 font-medium transition-all hover:underline [&[data-state=open]>svg]:rotate-180',\n    65→        className\n    66→      )}\n    67→      {...props}\n    68→    >\n    69→      {children}\n    70→      <ChevronDown className=\"h-4 w-4 shrink-0 transition-transform duration-200\" />\n    71→    </AccordionPrimitive.Trigger>\n    72→  </AccordionPrimitive.Header>\n    73→));\n    74→AccordionTrigger.displayName = AccordionPrimitive.Trigger.displayName;\n    75→\n    76→const AccordionContent = React.forwardRef<\n    77→  React.ElementRef<typeof AccordionPrimitive.Content>,\n    78→  React.ComponentPropsWithoutRef<typeof AccordionPrimitive.Content>\n    79→>(({ className, children, ...props }, ref) => (\n    80→  <AccordionPrimitive.Content\n    81→    ref={ref}\n    82→    className=\"overflow-hidden text-sm transition-all data-[state=closed]:animate-accordion-up data-[state=open]:animate-accordion-down\"\n    83→    {...props}\n    84→  >\n    85→    <div className={cn('pb-4 pt-0', className)}>{children}</div>\n    86→  </AccordionPrimitive.Content>\n    87→));\n    88→\n    89→AccordionContent.displayName = AccordionPrimitive.Content.displayName;\n    90→\n    91→export { Accordion, AccordionItem, AccordionTrigger, AccordionContent };\n    92→\n\n<system-reminder>