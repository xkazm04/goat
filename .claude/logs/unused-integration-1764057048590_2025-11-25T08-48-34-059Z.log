olute pointer-events-none';\n    93â†’\n    94â†’    switch (shape) {\n    95â†’      case 'circle':\n    96â†’        return (\n    97â†’          <div\n    98â†’            className={`${baseClasses} rounded-full`}\n    99â†’            style={{\n   100â†’              width: size,\n   101â†’              height: size,\n   102â†’              background: color,\n   103â†’              boxShadow: `0 0 10px ${color}`,\n   104â†’            }}\n   105â†’          />\n   106â†’        );\n   107â†’      case 'square':\n   108â†’        return (\n   109â†’          <div\n   110â†’            className={baseClasses}\n   111â†’            style={{\n   112â†’              width: size,\n   113â†’              height: size,\n   114â†’              background: color,\n   115â†’              boxShadow: `0 0 10px ${color}`,\n   116â†’            }}\n   117â†’          />\n   118â†’        );\n   119â†’      case 'triangle':\n   120â†’        return (\n   121â†’          <div\n   122â†’            className={baseClasses}\n   123â†’            style={{\n   124â†’              width: 0,\n   125â†’              height: 0,\n   126â†’              borderLeft: `${size / 2}px solid transparent`,\n   127â†’              borderRight: `${size / 2}px solid transparent`,\n   128â†’              borderBottom: `${size}px solid ${color}`,\n   129â†’              filter: `drop-shadow(0 0 5px ${color})`,\n   130â†’            }}\n   131â†’          />\n   132â†’        );\n   133â†’      case 'star':\n   134â†’        return (\n   135â†’          <div\n   136â†’            className={baseClasses}\n   137â†’            style={{\n   138â†’              width: size,\n   139â†’              height: size,\n   140â†’              background: color,\n   141â†’              clipPath: 'polygon(50% 0%, 61% 35%, 98% 35%, 68% 57%, 79% 91%, 50% 70%, 21% 91%, 32% 57%, 2% 35%, 39% 35%)',\n   142â†’              filter: `drop-shadow(0 0 5px ${color})`,\n   143â†’            }}\n   144â†’          />\n   145â†’